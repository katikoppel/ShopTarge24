@model Guid

@{
	ViewData["Title"] = "Not Found";
}

<div class="text-center mt-5">
	<h1 class="text-danger">404 - Not Found</h1>
	<p>@Model</p>
	<p>The item you are looking for does not exist or has been removed.</p>

	<a asp-action="Index" asp-controller="Home" class="btn btn-primary">Home</a>
</div>
